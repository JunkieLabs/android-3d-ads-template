<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_labs"
    app:startDestination="@id/labsMainFragment">


    <action android:id="@+id/navigate_global_to_navDefault"
        app:destination="@id/nav_default"/>

    <action android:id="@+id/navigate_global_to_labsMainFragment"
        app:destination="@id/labsMainFragment"/>

    <fragment
        android:id="@+id/labsMainFragment"
        android:name="in.junkielabs.adsmeta.ui.labs.LabsFragmentMain"
        android:label="Main"
        tools:layout="@layout/labs_main_fragment">

        <action android:id="@+id/navigate_to_labsSample1"
            app:destination="@id/labsSample1Fragment"/>

        <action android:id="@+id/navigate_to_labs3d"
            app:destination="@id/labs3dFragment"/>

        <action android:id="@+id/navigate_to_labs3d_sense"
            app:destination="@id/labs3dSenseFragment"/>

    </fragment>

    <fragment
        android:id="@+id/labsSample1Fragment"
        android:name="in.junkielabs.adsmeta.ui.labs.sample1.LabsFragmentSample1"
        android:label="@string/first_fragment_label"
        tools:layout="@layout/labs_sample1_fragment">


    </fragment>

    <fragment
        android:id="@+id/labs3dFragment"
        android:name="in.junkielabs.adsmeta.ui.labs.three.LabsFragment3d"
        android:label="3d"
        tools:layout="@layout/labs_3d_fragment">


    </fragment>

    <fragment
        android:id="@+id/labs3dSenseFragment"
        android:name="in.junkielabs.adsmeta.ui.labs.three.sense.LabsFragment3dSense"
        android:label="3d Sense"
        tools:layout="@layout/labs_3dsense_fragment">


    </fragment>


</navigation>